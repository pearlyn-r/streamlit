start_row_excel = 2 # Start writing from Excel row 2 (e.g., if row 1 is a header)

for row_idx, row in enumerate(extracted_data, start=start_row_excel):
    for col_idx, value in enumerate(row):
        excel_col_idx = col_idx + 1
        current_cell = sheet1.cell(row=row_idx, column=excel_col_idx)

        if col_idx in [3, 4, 5]: # Columns 4, 5, 6 (0-indexed 3, 4, 5)
            try:
                num = float(str(value).strip())
                current_cell.value = num
                current_cell.number_format = '#,##0.00'
            except ValueError:
                current_cell.value = value
        else:
            current_cell.value = value
